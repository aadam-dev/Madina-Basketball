# Supabase Configuration
# Get these from your Supabase project: Settings â†’ API
NEXT_PUBLIC_SUPABASE_URL=https://your-project-id.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here

# Admin Credentials
# Set secure passwords for admin accounts
ADMIN_EMAIL_1=admin1@example.com
ADMIN_PASSWORD_1=your_secure_password_1
ADMIN_EMAIL_2=admin2@example.com
ADMIN_PASSWORD_2=your_secure_password_2

# Optional: Bcrypt Hashed Passwords (Recommended for Production)
# Generate hashes using: node -e "const bcrypt = require('bcryptjs'); console.log(bcrypt.hashSync('your_password', 10))"
# ADMIN_PASSWORD_1_HASH=$2a$10$...
# ADMIN_PASSWORD_2_HASH=$2a$10$...

# Node Environment
# Automatically set by Vercel/Next.js, but you can override
# NODE_ENV=production
